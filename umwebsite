                isNew: true,
                colors: ["Black", "Brown", "Tan"]
            }
        ];

        const heroImages = [
            "https://images.unsplash.com/photo-1523398002811-999ca8dec234?crop=entropy&cs=srgb&fm=jpg&ixid=M3w3NTY2NzZ8MHwxfHNlYXJjaHwxfHxzdHJlZXR3ZWFyJTIwZmFzaGlvbnxlbnwwfHx8fDE3NTc0NTkxMzh8MA&ixlib=rb-4.1.0&q=85",
            "https://images.unsplash.com/photo-1588117260148-b47818741c74?crop=entropy&cs=srgb&fm=jpg&ixid=M3w3NTY2NzZ8MHwxfHxzdHJlZXR3ZWFyJTIwZmFzaGlvbnxlbnwwfHx8fDE3NTc0NTkxMzh8MA&ixlib=rb-4.1.0&q=85",
            "https://images.unsplash.com/flagged/photo-1562053690-62f7812fd0de?crop=entropy&cs=srgb&fm=jpg&ixid=M3w3NDk1ODB8MHwxfHx1cmJhbiUyMHN0eWxlfGVufDB8fHx8MTc1NzQ1OTE0NHww&ixlib=rb-4.1.0&q=85"
        ];
        
        // Global state
        let cartItems = [];
        let wishlistItems = [];
        let currentHeroImage = 0;
        let activeFilter = 'all';

        // DOM elements
        const mobileMenuBtn = document.getElementById('mobileMenuBtn');
        const navigation = document.getElementById('navigation');
        const heroImagesContainer = document.getElementById('heroImages');
        const heroIndicators = document.getElementById('heroIndicators');
        const productGrid = document.getElementById('productGrid');
        const cartBadge = document.getElementById('cartBadge');
        const wishlistBadge = document.getElementById('wishlistBadge');
        const searchInput = document.getElementById('searchInput');

        // Initialize the website
        document.addEventListener('DOMContentLoaded', function() {
            initHeroSlider();
            initProductGrid();
            initEventListeners();
            updateBadges();
        });

        // Mobile menu toggle
        function initEventListeners() {
            if (mobileMenuBtn && navigation) {
                mobileMenuBtn.addEventListener('click', function() {
                    navigation.classList.toggle('show');
                });
            }

            // Filter buttons
            const filterButtons = document.querySelectorAll('.filter-btn');
            filterButtons.forEach(btn => {
                btn.addEventListener('click', function() {
                    filterButtons.forEach(b => b.classList.remove('active'));
                    this.classList.add('active');
                    
                    activeFilter = this.dataset.filter;
                    renderProducts();
                });
            });

            // Search functionality
            if (searchInput) {
                searchInput.addEventListener('input', function() {
                    renderProducts();
                });
            }
        }

        // Hero slider functionality
        function initHeroSlider() {
            if (heroImagesContainer && heroIndicators) {
                heroImages.forEach((image, index) => {
                    const heroDiv = document.createElement('div');
                    heroDiv.className = `hero-image ${index === 0 ? 'active' : ''}`;
                    heroDiv.innerHTML = `<img src="${image}" alt="Hero ${index + 1}">`;
                    heroImagesContainer.appendChild(heroDiv);
                });

                heroImages.forEach((_, index) => {
                    const indicator = document.createElement('button');
                    indicator.className = `hero-indicator ${index === 0 ? 'active' : ''}`;
                    indicator.addEventListener('click', () => setHeroImage(index));
                    heroIndicators.appendChild(indicator);
                });

                setInterval(() => {
                    currentHeroImage = (currentHeroImage + 1) % heroImages.length;
                    setHeroImage(currentHeroImage);
                }, 5000);
            }
        }

        function setHeroImage(index) {
            const heroImageElements = document.querySelectorAll('.hero-image');
            const indicatorElements = document.querySelectorAll('.hero-indicator');
            
            heroImageElements.forEach(img => img.classList.remove('active'));
            indicatorElements.forEach(ind => ind.classList.remove('active'));
            
            heroImageElements[index].classList.add('active');
            indicatorElements[index].classList.add('active');
            
            currentHeroImage = index;
        }

        // Product grid functionality
        function initProductGrid() {
            renderProducts();
        }

        function renderProducts() {
            if (!productGrid) return;
            const searchTerm = searchInput ? searchInput.value.toLowerCase() : '';
            
            let filteredProducts = mockProducts.filter(product => {
                const categoryMatch = activeFilter === 'all' 
                    ? product.isNew 
                    : product.category === activeFilter;
                
                const searchMatch = product.name.toLowerCase().includes(searchTerm);
                
                return categoryMatch && searchMatch;
            });

            productGrid.innerHTML = '';

            filteredProducts.forEach(product => {
                const productCard = createProductCard(product);
                productGrid.appendChild(productCard);
            });
        }

        function createProductCard(product) {
            const card = document.createElement('div');
            card.className = 'product-card';
            
            const colorOptions = product.colors.map(color => {
                const colorClass = `color-${color.toLowerCase().replace(' ', '-')}`;
                return `<div class="color-option ${colorClass}" title="${color}"></div>`;
            }).join('');

            card.innerHTML = `
                <div class="product-image">
                    <img src="${product.image}" alt="${product.name}">
                    
                    <button class="wishlist-btn ${isInWishlist(product.id) ? 'active' : ''}" onclick="toggleWishlist(${product.id})">
                        <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                            <path d="M20.84 4.61a5.5 5.5 0 0 0-7.78 0L12 5.67l-1.06-1.06a5.5 5.5 0 0 0-7.78 7.78l1.06 1.06L12 21.23l7.78-7.78 1.06-1.06a5.5 5.5 0 0 0 0-7.78z"></path>
                        </svg>
                    </button>

                    <button class="quick-add-btn" onclick="addToCart(${product.id})">
                        <svg width="14" height="14" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                            <path d="M9 22C9.55228 22 10 21.5523 10 21C10 20.4477 9.55228 20 9 20C8.44772 20 8 20.4477 8 21C8 21.5523 8.44772 22 9 22Z"></path>
                            <path d="M20 22C20.5523 22 21 21.5523 21 21C21 20.4477 20.5523 20 20 20C19.4477 20 19 20.4477 19 21C19 21.5523 19.4477 22 20 22Z"></path>
                            <path d="M1 1H5L7.68 14.39C7.77144 14.8504 8.02191 15.264 8.38755 15.5583C8.75318 15.8526 9.2107 16.009 9.68 16H19.4C19.8693 16.009 20.3268 15.8526 20.6925 15.5583C21.0581 15.264 21.3086 14.8504 21.4 14.39L23 6H6"></path>
                        </svg>
                        <span>QUICK ADD</span>
                    </button>

                    ${product.isNew ? '<div class="new-badge">NEW</div>' : ''}
                </div>

                <div class="product-info">
                    <h3>${product.name}</h3>
                    <div class="product-price">
                        <span class="current-price">â¹${product.price}</span>
                        ${product.originalPrice > product.price ? 
                            `<span class="original-price">â¹${product.originalPrice}</span>` : ''}
                    </div>
                    <div class="color-options">
                        ${colorOptions}
                    </div>
                </div>
            `;

            return card;
        }

        // Cart functionality
        function addToCart(productId) {
            const product = mockProducts.find(p => p.id === productId);
            if (!product) return;

            const existingItem = cartItems.find(item => item.id === productId);
            if (existingItem) {
                existingItem.quantity += 1;
            } else {
                cartItems.push({ ...product, quantity: 1 });
            }

            updateBadges();
            showNotification(`${product.name} added to cart!`);
        }

        // Wishlist functionality
        function toggleWishlist(productId) {
            const product = mockProducts.find(p => p.id === productId);
            if (!product) return;

            const existingIndex = wishlistItems.findIndex(item => item.id === productId);
            if (existingIndex !== -1) {
                wishlistItems.splice(existingIndex, 1);
                showNotification(`${product.name} removed from wishlist!`);
            } else {
                wishlistItems.push(product);
                showNotification(`${product.name} added to wishlist!`);
            }

            updateBadges();
            renderProducts(); // Re-render to update wishlist button states
        }

        function isInWishlist(productId) {
            return wishlistItems.some(item => item.id === productId);
        }

        // Update badge counters
        function updateBadges() {
            const cartCount = cartItems.reduce((total, item) => total + item.quantity, 0);
            const wishlistCount = wishlistItems.length;

            if (cartBadge) {
                if (cartCount > 0) {
                    cartBadge.textContent = cartCount;
                    cartBadge.classList.add('show');
                } else {
                    cartBadge.classList.remove('show');
                }
            }

            if (wishlistBadge) {
                if (wishlistCount > 0) {
                    wishlistBadge.textContent = wishlistCount;
                    wishlistBadge.classList.add('show');
                } else {
                    wishlistBadge.classList.remove('show');
                }
            }
        }

        // Notification system
        function showNotification(message) {
            // Create notification element
            const notification = document.createElement('div');
            notification.style.cssText = `
                position: fixed;
                top: 20px;
                right: 20px;
                background: #000;
                color: white;
                padding: 12px 24px;
                border-radius: 4px;
                font-family: 'Poppins', sans-serif;
                font-weight: 700;
                font-size: 14px;
                z-index: 10000;
                opacity: 0;
                transform: translateY(-20px);
                transition: all 0.3s ease;
            `;
            notification.textContent = message;

            document.body.appendChild(notification);

            // Animate in
            setTimeout(() => {
                notification.style.opacity = '1';
                notification.style.transform = 'translateY(0)';
            }, 100);

            // Remove after 3 seconds
            setTimeout(() => {
                notification.style.opacity = '0';
                notification.style.transform = 'translateY(-20px)';
                setTimeout(() => {
                    document.body.removeChild(notification);
                }, 300);
            }, 3000);
        }

        // Smooth scrolling for internal links
        document.addEventListener('click', function(e) {
            if (e.target.classList.contains('shop-now-btn')) {
                e.preventDefault();
                const productsSection = document.querySelector('.products');
                if (productsSection) {
                    productsSection.scrollIntoView({
                        behavior: 'smooth'
                    });
                }
            }
        });

        // Close mobile menu when clicking outside
        document.addEventListener('click', function(e) {
            if (navigation && mobileMenuBtn && !navigation.contains(e.target) && !mobileMenuBtn.contains(e.target)) {
                navigation.classList.remove('show');
            }
        });
    </script>
</body>
</html>
